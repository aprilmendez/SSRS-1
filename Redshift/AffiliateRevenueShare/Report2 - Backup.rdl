        </Field>
        <Field Name="status">
          <DataField>status</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="subscription_revenue">
          <DataField>subscription revenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="billingplanid">
          <DataField>billingplanid</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Redshift_psqlODBC_shipstation</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
	s.SellerID+110000 as "ShipStation Seller",  
	s.CompanyName AS "Company Name",
	s.CreateDate AS "Registered",
	MAX(st.AccountName) AS "BigCommerce Domain", 
	CASE
		WHEN COUNT(1) = 1 THEN ''
		ELSE CONVERT(VARCHAR,COUNT(1)-1)
	END AS "Number of Additional Stores",
	p.PricePerMonth AS "Subscription Level",
	CASE
		WHEN datediff(day,s.CreateDate,trunc(getdate())) &gt; 30 THEN 'Paid'
		ELSE 'Trial'
	END AS "Status",
	CASE
		WHEN datediff(day,s.CreateDate,trunc(getdate())) &gt; 30 THEN p.PricePerMonth
		ELSE 0.0
	END AS "Subscription Revenue"
	
	, s.billingplanid
FROM
	Seller s
	INNER JOIN Store st ON s.SellerID = st.SellerID
	INNER JOIN BillingPlan p ON s.BillingPlanID = p.BillingPlanID
WHERE
	st.MarketplaceID = 13
	AND st.Active = 1
	AND s.CreateDate &gt; '2/28/2013'
	AND s.CreateDate &lt; trunc(getdate())
	AND (s.CancelDate IS NULL OR (s.CancelDate &gt; add_months(trunc(getdate()), -1) AND s.CancelDate &lt; trunc(getdate())))
GROUP BY 
	s.SellerID, s.CompanyName, s.CreateDate, p.PricePerMonth
	, s.billingplanid
ORDER BY 
	s.CreateDate, s.CompanyName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
