      <Fields>
        <Field Name="companyname">
          <DataField>companyname</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="registereddate">
          <DataField>registereddate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="storename">
          <DataField>storename</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="additionalstores">
          <DataField>additionalstores</DataField>
          <rd:TypeName>System.Int64</rd:TypeName>
        </Field>
        <Field Name="subscriptionrevenue">
          <DataField>subscriptionrevenue</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="accounttype">
          <DataField>accounttype</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="affiliatecommission">
          <DataField>affiliatecommission</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Redshift</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
	s.CompanyName AS CompanyName,
	trunc(s.CreateDate) AS RegisteredDate,
	MAX(st.AccountName) AS StoreName,  
	CASE
		WHEN COUNT(*) = 1 THEN 0
		ELSE COUNT(*)
	END AS AdditionalStores,
	CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 0.00  -- Last day of previous month
		ELSE p.PricePerMonth 
	END AS SubscriptionRevenue,
	CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 'Trial'  -- Last day of previous month
		ELSE 'Paid'
	END AS AccountType,
	to_number(CASE
		WHEN datediff(day,s.CreateDate,?) &lt;= 30 OR p.billingplanid = 0 THEN 0.00   -- Last day of previous month
		ELSE p.PricePerMonth * ?  -- Commission Percent
	END, '999,999D99') AS AffiliateCommission
FROM
	Seller s
	INNER JOIN Store st ON s.SellerID = st.SellerID
	INNER JOIN BillingPlan p ON s.BillingPlanID = p.BillingPlanID
WHERE
	st.MarketplaceID = ?    -- Marketplace Id
	AND st.Active = 1
	AND p.Active = 1
	AND s.CreateDate &gt; '2/28/2013'
	AND s.CreateDate &lt;= ?    -- Last day of previous month
	AND (s.CancelDate IS NULL OR (s.CancelDate &gt; add_months(?, -1) AND s.CancelDate &lt;= ?))   -- Last day of previous month
GROUP BY 
	  s.CompanyName
	, s.CreateDate
	, p.PricePerMonth
	, st.marketplaceid
	, p.billingplanid
ORDER BY 
	s.CreateDate, s.CompanyName</CommandText>
